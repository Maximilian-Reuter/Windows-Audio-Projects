<Window x:Class="Audiotest2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Audiotest2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Height="29" Margin="90,53,0,0" VerticalAlignment="Top" Width="319"/>
        <ComboBox x:Name="comboBox_Copy" HorizontalAlignment="Left" Height="29" Margin="90,102,0,0" VerticalAlignment="Top" Width="319"/>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Height="41" Margin="90,238,0,0" VerticalAlignment="Top" Width="123" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Stop" HorizontalAlignment="Left" Height="41" Margin="286,238,0,0" VerticalAlignment="Top" Width="123" Click="button_Copy_Click"/>

    </Grid>
</Window>
